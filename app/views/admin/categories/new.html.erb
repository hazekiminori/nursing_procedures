  <div class="container">
    <h4>カテゴリー作成</h4>
      <%= form_with model: @category, url: admin_categories_path, method: :post do |f| %>
        <%= f.text_field :name %>
        <%= f.submit "作成する" %>　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　
      <% end %>
      
      <%= link_to "カテゴリー一覧に戻る", admin_categories_path %>
  </div>