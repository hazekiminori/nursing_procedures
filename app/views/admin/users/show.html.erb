<div class = "container">
  <h4>利用者詳細</h4>
    <p>職員番号</p>
    <p><%= @user.staff_number %></p>
    <p>氏名</p>
    <p><%= @user.name %></p>
    <p>会員ステータス</p>
      <% if @user.is_deleted != false %>　
        <p>無効</p>
      <% else %>
        <p>有効</p>
      <% end %>
  
  <%= link_to "利用者情報編集", edit_admin_user_path %>

    <% @user.procedures.each do |procedure| %>
      <%= procedure.title %>
    <% end %>
</div>